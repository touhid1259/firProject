<div class='container padding-0'>
	<div class='jumbotron'>
		<div class="row">

			<div class="col-md-12">
				<h2 class="preferred-schedule"> Summary and Statistics: </h2>
			</div>

			<div class="col-md-12 padding-0">

        <div class="col-md-6 padding-0">
  				<h4 class="schedule-choices"> Optimal Schedule </h4>
  				<div class="col-md-12 optimal-graph">
  				</div>
  			</div>

  			<div class="col-md-6 padding-0">
  				<h4 class="schedule-choices"> Statistics: </h4>
					<div class="col-md-12">
							<h5 class="h5-schedule-choices"> Current Belt One Velocity (v1)<%= raw(repeat_html_code(3, "&nbsp;")) %>: <%= rand(10..30) %> km/hour </h5>
							<h5 class="h5-schedule-choices"> Current Belt Two Velocity (v2)<%= raw(repeat_html_code(3, "&nbsp;")) %>: <%= rand(10..30) %> km/hour </h5>
							<h5 class="h5-schedule-choices"> Current Energy Price<%= raw(repeat_html_code(17, "&nbsp;")) %>: <img src="<%= asset_path('euro_symbol.png') %>" class="euro-sign"> <%= rand(25..35) %> per unit</h5>
							<h5 class="h5-schedule-choices"> Current Energy Consumption<%= raw(repeat_html_code(4, "&nbsp;")) %>: <%= rand(40..60) %> kWh</h5>
							<h5 class="h5-schedule-choices"> User Status : </h5>
							<% sentiment = ["sentiment_satisfied", "sentiment_dissatisfied", "sentiment_neutral"] %>
							<h4> <i class="material-icons user-status"><%= sentiment[rand(0..2)] %></i> </h4>
					</div>
  			</div>

      </div>

      <div class="col-md-12 padding-0" style="margin-top: 30px;">

        <div class="col-md-6 padding-0">
          <h4 class="schedule-choices"> Energy Consumption </h4>
          <div class="col-md-12 energy-consumption-graph">
          </div>
        </div>

        <div class="col-md-6 padding-0">
          <h4 class="schedule-choices"> Energy Price </h4>
          <div class="col-md-12 energy-price-graph">
          </div>
        </div>

      </div>

		</div>
	</div>
</div>
